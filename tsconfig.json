{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2022",
        "jsx": "react",
        "checkJs": true,
        "allowJs": true,
        "noEmit": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "sourceMap": true,
        "verbatimModuleSyntax": true,
        "isolatedModules": true,
        "resolveJsonModule": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}